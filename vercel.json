{
  "rewrites": [
    {
      "source": "/:code",
      "destination": "/api/[code]"
    }
  ],
  "headers": [
    {
      "source": "/:code",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}